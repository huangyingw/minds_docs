[remote "Minds"]
    url = git@gitlab.com:minds/docs.git 
    fetch = +refs/heads/*:refs/remotes/Minds/*
[push]
    remote = origin
[gsync]
    remote = Minds
    branch = master
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[remote "origin"]
	url = git@github.com:huangyingw/minds_docs.git 
	fetch = +refs/heads/*:refs/remotes/origin/*
[gdif]
    ignore = *.gdio.diff
